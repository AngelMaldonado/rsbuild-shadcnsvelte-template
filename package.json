{
  "name": "rsbuild-svelte-template",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "start": "rsbuild build && bun run server.ts",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "format": "biome format --write",
    "preview": "rsbuild preview",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "commit": "cz",
    "release": "release-it"
  },
  "dependencies": {
    "@lucide/svelte": "^0.542.0",
    "clsx": "^2.1.1",
    "svelte": "^5.38.6",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tw-animate-css": "^1.3.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@rsbuild/core": "^1.5.2",
    "@rsbuild/plugin-svelte": "^1.0.11",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bun": "^1.2.21",
    "cz-git": "^1.12.0",
    "release-it": "^19.0.4",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
